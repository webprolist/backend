{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    // "moduleResolution": "node",
    "baseUrl": "src",
    "outDir": "dist",
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    // "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
  }
}